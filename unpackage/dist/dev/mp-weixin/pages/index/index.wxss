}
.form .subTitle {
  padding: 20rpx;
  font-size: 32rpx;
  font-weight: 700;
  display: flex;
  justify-content: space-between;
  color: #7f7e7e;
}
.form .subTitle .txt {
  position: relative;
  padding-left: 20rpx;
}
.form .subTitle .txt:before {
  content: '';
  width: 10rpx;
  height: 30rpx;
  background-color: #028f4b;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  margin: auto;
}
.form .info {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}
.form .info .item-info {
  width: 40%;
  height: 200rpx;
  border-radius: 30rpx;
  margin: 30rpx;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}
.form .info .item-info .item-info-img {
  border-radius: 30rpx;
  height: 100%;
}
.form .info .item-info .txt {
  color: red;
  position: absolute;
  right: 30rpx;
  bottom: 8rpx;
}
.form .introduction {
  width: 90%;
  height: 300rpx;
  border-radius: 30rpx;
  margin: 20rpx auto 140rpx auto;
  text-align: center;
}
.form .introduction .introduction-img {
  height: 300rpx;
  border-radius: 30rpx;
}
.form .introduction .txt {
  display: block;
  color: #7f7e7e;
}

